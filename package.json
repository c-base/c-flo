{
  "name": "c-flo",
  "version": "1.0.0",
  "description": "MsgFlo setup for the c-base space station",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "postinstall": "./node_modules/.bin/noflo-cache-preheat",
    "start": "./node_modules/.bin/msgflo --graph graphs/c-base.json --auto-save=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-base/c-flo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-base/c-flo/issues"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "homepage": "https://github.com/c-base/c-flo#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.13.2",
    "grunt-noflo-manifest": "^0.1.11",
    "grunt-yamllint": "^0.3.0",
    "ipfsd-ctl": "^0.20.0",
    "mocha": "^3.2.0",
    "noflo-nodejs": "^0.8.0"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "ipfs-api": "^14.0.0",
    "msgflo": "^0.10.1",
    "msgflo-nodejs": "^0.10.2",
    "needle": "^1.0.0",
    "noflo": "^0.8.3",
    "noflo-core": "^0.2.3",
    "noflo-runtime-msgflo": "~0.11.2"
  },
  "msgflo": {
    "namespace": "c-base",
    "handlers": {
      ".yml": "node ./node_modules/.bin/msgflo-register-foreign #FILENAME --forever=true"
    },
    "variables": {
      "NOFLO": "node ./node_modules/.bin/noflo-runtime-msgflo --name #ROLE --graph #COMPONENT  --cache true"
    },
    "components": {
      "c-flo/DetectPartyMods": "#NOFLO",
      "c-flo/DownloadToIpfs": "#NOFLO",
      "core/Output": "#NOFLO"
    }
  },
  "noflo": {
    "components": {
      "DetectPartyMods": "components/DetectPartyMods.coffee",
      "DownloadToIpfs": "components/DownloadToIpfs.coffee"
    }
  }
}
