  "scripts": {
    "test": "grunt test",
    "postinstall": "./node_modules/.bin/noflo-cache-preheat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-base/c-flo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-base/c-flo/issues"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "homepage": "https://github.com/c-base/c-flo#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.13.2",
    "grunt-noflo-manifest": "^0.1.11",
    "grunt-yamllint": "^0.2.0",
    "ipfsd-ctl": "^0.18.2",
    "mocha": "^3.2.0",
    "noflo-nodejs": "^0.7.2"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "ipfs-api": "^12.0.2",
    "msgflo": "^0.10.1",
    "msgflo-nodejs": "^0.7.2",
    "needle": "^1.0.0",
    "noflo": "^0.7.9",
    "noflo-core": "^0.2.3",
    "noflo-runtime-msgflo": "~0.9.0"
  },
  "msgflo": {
    "variables": {
      "NOFLO": "node ./node_modules/.bin/noflo-runtime-msgflo --name #ROLE --graph #COMPONENT --iips #IIPS --cache true",
      "FOREIGN": "node ./node_modules/.bin/msgflo-register-foreign participants/#ROLE.yml"
    },
    "components": {
      "c-flo/DetectPartyMods": "#NOFLO",
      "c-flo/DownloadToIpfs": "#NOFLO",
      "c-flo/logger": "#NOFLO",
      "c-flo/ampel-nerd": "#FOREIGN",
      "c-flo/barstatus-bar": "#FOREIGN",
      "c-flo/barstatus-c-lab": "#FOREIGN",
      "c-flo/barstatus-nerd": "#FOREIGN",
      "c-flo/c_leuse": "#FOREIGN",
      "c-flo/c-beam": "#FOREIGN",
      "c-flo/he1": "#FOREIGN",
      "c-flo/ingress-table": "#FOREIGN",
      "c-flo/mechblast": "#FOREIGN",
      "c-flo/nerdctrl": "#FOREIGN",
      "c-flo/siri": "#FOREIGN"
    }
  },
  "noflo": {
    "components": {
      "DetectPartyMods": "components/DetectPartyMods.coffee",
      "DownloadToIpfs": "components/DownloadToIpfs.coffee"
    }
  }
}
